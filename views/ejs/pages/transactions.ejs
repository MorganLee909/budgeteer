<div id="transactionsPage" class="page" style="display:none">
    <div id="transactionsOptions">
        <h1>Filter</h1>

        <label>Date:
            <input id="filterDateFrom" type="date">
            -
            <input id="filterDateTo" type="date">
        </label>

        <div id="categoriesLabel">Categories</div>

        <label>Tags (comma seperated)
            <input id="filterTagsInput" type="text">
        </label>

        <template id="categoryChoice">
            <div class="categoryChoice">
                <input class="ccCheckbox" type="checkbox">
                <p class="ccName"></p>
            </div>
        </template>
    </div>

    <div id="transactionsDisplay">
        <h1>Transactions</h1>

        <table>
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Category</th>
                    <th>Location</th>
                    <th>Amount</th>
                </tr>
            </thead>
            <tbody id="searchTransactionsBody"></tbody>
        </table>

        <template id="tableTransaction">
            <tr class="tableTransaction actionable">
                <td class="ttDate"></td>
                <td class="ttCategory"></td>
                <td class="ttLocation"></td>
                <td class="ttAmount"></td>
            </tr>
        </template>
    </div>
</div>